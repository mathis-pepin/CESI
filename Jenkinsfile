pipeline {
  agent any
  stages {
    stage('TEST') {
      steps {
        echo 'debut test'
        sleep 30
        echo 'fin test'
      }
    }

    stage('BUILD') {
      steps {
        echo 'debut build'
        sleep 30
        echo 'fin build'
      }
    }

    stage('DEPLOY') {
      steps {
        echo 'début deploy'
        sleep 30
        echo 'fin deploy'
      }
    }

  }
}